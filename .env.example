# Server Configuration
PORT=8132

# Database Configuration
# Options: sqlite, postgres, mssql, mongodb
DB_TYPE=sqlite
DB_DSN=llm_proxy.db

# Master Connection (Used for auto-seeding on startup)
MASTER_CONN_NAME=Default-LLM
MASTER_CONN_PROVIDER=openai # openai, azure
MASTER_CONN_ENDPOINT=https://api.openai.com
MASTER_CONN_MODEL=gpt-4o
MASTER_CONN_API_KEY=your_real_api_key_here

# Default Virtual Key for Proxy Access
MASTER_VKEY_NAME=Default-Access
MASTER_VKEY_KEY=sk-proxy-default-key
MASTER_VKEY_TPS=5
MASTER_VKEY_TOKENS=10000

# Encryption Key (Base64 Encoded)
ENCRYPTION_KEY=replace-with-your-own-base64-key